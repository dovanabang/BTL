<script>
  var Idsp;
  var deleteForm = document.forms['delete-form'];
  const exampleModal = document.getElementById('delete-sphong');


  exampleModal.addEventListener('show.bs.modal', event => {
    const button = event.relatedTarget;
    
    Idsp = button.getAttribute('data-id');
    // console.log("\n\n\n\n\n\n" + id);
  });

  var btnDeleteSP = document.getElementById('btl-delete-sphong');

  btnDeleteSP.onclick = function() {
    //alert(Idsp);
    deleteForm.action = '/baohanh/sphong/delete/' + Idsp + "?_method=DELETE";
    deleteForm.submit();
  }   

 

  
</script>