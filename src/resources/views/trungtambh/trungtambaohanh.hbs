<div class="container-fluid">
    <h1>Danh mục các sản phẩm bảo hành:</h1>
    </div>
    
    <table class="table table-hover"> <!--bảng products -->
      <thead>
        <tr>
          <th scope="col">STT</th>
           <th scope="col">Mã Sản Phẩm</th> <!--produc_id -->
           <th scope="col">Trạng thái bảo hành</th> <!--produc_name -->
           <th scope="col">Số lần bảo hành</th>
        </tr>
      </thead>
      <tbody>
        {{#each Spbaohanh}}
        <tr>
          <th scope="row">{{sum @index 1}}</th>
          <th scope="row">{{this.Masp}}</th>
          <th scope="row">{{this.Trangthaibaohanh}}</th>
          <th scope="row">{{this.Solanbaohanh}}</th>
          
        </tr>
        {{/each}}
      </tbody>
    </table>
 
