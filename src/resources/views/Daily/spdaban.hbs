    <h1>Sản phẩm đã bán:</h1>
    </div>
    <div class="container-fluid">
      <a href="dailyphanphoi_btn_themsanphamdaban.html"><button type="button" class="btn btn-primary">Thêm sản phẩm</button></a>
     </div> 
    <table class="table table-hover"> <!--bảng products -->
      <thead>
        <tr>
          <th scope="col">STT</th>
          <th scope="col">Mã Sản phẩm</th>
          <th scope="col">Tên sản phẩm</th>
          <th scope="col">Mã đại lý</th> 
          <th scope="col">Ngày bán</th>
          <th scope="col">Mã khách hàng</th>
       </tr>
     </thead>
     <tbody>
       {{#each Chuaban}}
       <tr>
         <th scope="row">{{sum @index 1}}</th>
         <th scope="row">{{this.Masp}}</th>
         <th scope="row">{{this.Tensp}}</th>
         <th scope="row">{{this.Madaily}}</th>
         <th scope="row">{{this.Ngayban}}</th>
         <th scope="row">{{this.Makhachhang}}</th>
          
        </tr>
        {{/each}}
      </tbody>
    </table>
 
