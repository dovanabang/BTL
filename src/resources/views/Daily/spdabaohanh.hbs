          <th scope="col">Trạng thái bảo hành</th> <!--produc_id -->
       </tr>
     </thead>
     <tbody>
       {{#each Spdabaohanh}}
       <tr>
         <th scope="row">{{sum @index 1}}</th>
         <th scope="row">{{this.Masp}}</th>
         <th scope="row">{{this.Tensp}}</th>
         <th scope="row">{{this.Makho}}</th>
         <th scope="row">{{this.Ngaynhap}}</th>
          
        </tr>
        {{/each}}
      </tbody>
